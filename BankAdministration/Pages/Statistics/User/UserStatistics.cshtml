@page
@model BankAdministration.Pages.Statistics.User.UserStatisticsModel
@{
}

<div class="card">
    <div class="card-header">
        <h1>Alla användare</h1>
    </div>
    <div class="card-body">
        <table class="table">
            <tr>
                <th>Användar Id</th>
                <th>Användarnamn</th>
            </tr>
            @foreach (var user in Model.UserStatistics)
            {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.LoginName</td>
                </tr>
            }
        </table>
    </div>
</div>